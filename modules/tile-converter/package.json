{
  "name": "@loaders.gl/tile-converter",
  "version": "3.0.0-alpha.1",
  "description": "Converter",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "converter",
    "3dTiles",
    "i3s"
  ],
  "types": "src/index.d.ts",
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/es6/index.js",
  "sideEffects": false,
  "bin": {
    "tile-converter": "./bin/converter.js",
    "i3s-server": "./src/i3s-server/bin/www"
  },
  "files": [
    "src",
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-bin && npm run build-bundle && npm run build-bundle -- --env.dev",
    "build-bundle": "webpack --display=minimal --config ../../scripts/bundle.config.js",
    "build-bin": "BABEL_ENV=es5 babel scripts --config-file ../../babel.config.js --out-dir dist/scripts",
    "build-converter-bundle": "webpack --config ./converter-bundle.config.js"
  },
  "dependencies": {
    "@loaders.gl/3d-tiles": "3.0.0-alpha.1",
    "@loaders.gl/core": "3.0.0-alpha.1",
    "@loaders.gl/crypto": "3.0.0-alpha.1",
    "@loaders.gl/draco": "3.0.0-alpha.1",
    "@loaders.gl/gltf": "3.0.0-alpha.1",
    "@loaders.gl/i3s": "3.0.0-alpha.1",
    "@loaders.gl/images": "3.0.0-alpha.1",
    "@loaders.gl/loader-utils": "3.0.0-alpha.1",
    "@loaders.gl/polyfills": "3.0.0-alpha.1",
    "@loaders.gl/tiles": "3.0.0-alpha.1",
    "@math.gl/core": "^3.3.0",
    "@math.gl/culling": "^3.3.0",
    "@math.gl/geospatial": "^3.3.0",
    "archiver": "^5.0.0",
    "cors": "^2.8.5",
    "crypt": "^0.0.2",
    "debug": "~2.6.9",
    "draco3d": "1.4.1",
    "express": "~4.16.1",
    "json-map-transform": "^1.2.6",
    "jszip": "^3.5.0",
    "md5": "^2.3.0",
    "morgan": "~1.9.1",
    "probe.gl": "^3.3.0",
    "uuid": "^8.1.0"
  }
}
